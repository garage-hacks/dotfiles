[core]
  editor = vim
  excludesfile = ~/.gitignoroe
  quotepath = false
  precomposeunicode = true

[push]
	default = simple

[diff]
  algorithm = patience

[alias]
  a = add
  s = status
  co = checkout
  cm = commit
  cmam = commit -am
  cmm = commit -m
  br = branch
  rmc = rm --cached
  log = log -p
  l = log
  log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  log-all = log --graph --all --color --pretty='%x09%h %cn%x09%s %Cred%d%Creset'

[user]
	name = chansuke
	email = moonset20@gmail.com
